{
	"name": "quick-work-taro-app",
	"version": "1.0.0",
	"private": true,
	"description": "这是一个小程序模板",
	"templateInfo": {
		"name": "mobx",
		"typescript": true,
		"css": "sass"
	},
	"main": "index.js",
	"scripts": {
		"start": "npm run dev:weapp",
		"build": "npm run build:weapp",
		"copy-tabbar": "cp -r ./src/custom-tab-bar ./dist/custom-tab-bar",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch"
	},
	"author": "Mark",
	"license": "MIT",
	"dependencies": {
		"@tarojs/async-await": "^2.0.0-beta.12",
		"@tarojs/components": "1.3.29",
		"@tarojs/mobx": "1.3.29",
		"@tarojs/mobx-h5": "1.3.29",
		"@tarojs/mobx-rn": "1.3.29",
		"@tarojs/rn-runner": "1.3.29",
		"@tarojs/router": "1.3.29",
		"@tarojs/taro": "1.3.29",
		"@tarojs/taro-alipay": "1.3.29",
		"@tarojs/taro-h5": "1.3.29",
		"@tarojs/taro-swan": "1.3.29",
		"@tarojs/taro-tt": "1.3.29",
		"@tarojs/taro-weapp": "1.3.29",
		"eslint": "^6.7.2",
		"mobx": "4.8.0",
		"nerv-devtools": "^1.5.6",
		"nervjs": "^1.5.6",
		"taro-ui": "^2.2.4"
	},
	"devDependencies": {
		"@tarojs/plugin-babel": "1.3.29",
		"@tarojs/plugin-csso": "1.3.29",
		"@tarojs/plugin-sass": "1.3.29",
		"@tarojs/plugin-uglifyjs": "1.3.29",
		"@tarojs/webpack-runner": "1.3.29",
		"@types/react": "16.9.13",
		"@types/webpack-env": "^1.14.1",
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"babel-eslint": "^10.0.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.8",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"eslint-config-taro": "1.3.29",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"eslint-plugin-taro": "1.3.29",
		"stylelint": "11.1.1",
		"stylelint-config-taro-rn": "1.3.29",
		"stylelint-taro-rn": "1.3.29",
		"typescript": "^3.7.2"
	}
}
